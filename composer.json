{
    "name": "tienvx/mbt-bundle",
    "type": "symfony-bundle",
    "description": "Provide tools to test a system using Model Based Testing technique.",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
          "name": "Tien Xuan Vo",
          "email": "tien.xuan.vo@gmail.com",
          "role": "Developer"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/tienvx/TienvxMbtBundle/contributors"
        }
    ],
    "require": {
        "api-platform/core": "^2.2",
        "graphp/algorithms": "^0.8.1",
        "php": "^7.1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/console": "^4.0",
        "symfony/process": "^4.0",
        "symfony/workflow": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/templating": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/asset": "^4.0",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/browser-kit": "^4.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\Tests\\": ["tests", "tests/app/src"]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<4.0",
        "symfony/debug": "<4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
