{
    "name": "tienvx/mbt-bundle",
    "type": "symfony-bundle",
    "description": "Provide tools to test a system using Model Based Testing technique.",
    "keywords": ["model based testing", "testing tools"],
    "license": "MIT",
    "authors": [
        {
          "name": "Tien Xuan Vo",
          "email": "tien.xuan.vo@gmail.com",
          "role": "Developer"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/tienvx/mbt-bundle/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "graphp/algorithms": "~0.8.1",
        "symfony/framework-bundle": "~4.4.0",
        "symfony/console": "~4.4.0",
        "symfony/workflow": "~4.4.0",
        "symfony/validator": "~4.4.0",
        "symfony/messenger": "4.4.x-dev",
        "symfony/property-info": "~4.4.0",
        "doctrine/doctrine-bundle": "~2.0.0",
        "doctrine/orm": "~2.7.0",
        "league/flysystem-bundle": "~1.2.2"
    },
    "require-dev": {
        "symfony/yaml": "~4.4.0",
        "symfony/expression-language": "~4.4.0",
        "symfony/security-bundle": "~4.4.0",
        "symfony/maker-bundle": "~1.14.3",
        "league/flysystem-memory": "~1.0.2"
    },
    "autoload": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\Tests\\": "tests/",
            "App\\": "tests/app/src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.15-dev"
        }
    },
    "suggest": {
        "ext-amqp": "Needed to support amqp transport",
        "alek13/slack": "Send bug report to Slack",
        "symfony/mailer": "Send bug report to an email",
        "symfony/twig-bundle": "Render bug reporter in html format, for email reporting",
        "symfony/mime": "Create email"
    }
}
