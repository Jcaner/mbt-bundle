{
    "name": "tienvx/mbt-bundle",
    "type": "symfony-bundle",
    "description": "Provide tools to test a system using Model Based Testing technique.",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
          "name": "Tien Xuan Vo",
          "email": "tien.xuan.vo@gmail.com",
          "role": "Developer"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/tienvx/TienvxMbtBundle/contributors"
        }
    ],
    "require": {
        "graphp/algorithms": "^0.8.1",
        "php": "^7.1.0",
        "symfony/console": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/workflow": "^3.3",
        "symfony/yaml": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/templating": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.5",
        "symfony/process": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/browser-kit": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
