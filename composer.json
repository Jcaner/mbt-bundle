{
    "name": "tienvx/mbt-bundle",
    "type": "symfony-bundle",
    "description": "Provide tools to test a system using Model Based Testing technique.",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
          "name": "Tien Xuan Vo",
          "email": "tien.xuan.vo@gmail.com",
          "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "symfony/console": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/workflow": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tienvx\\bundle\\MbtBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\bundle\\MbtBundle\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
