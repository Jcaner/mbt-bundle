{
    "name": "tienvx/mbt-bundle",
    "type": "symfony-bundle",
    "description": "Provide tools to test a system using Model Based Testing technique.",
    "keywords": ["model based testing", "testing tools"],
    "license": "MIT",
    "authors": [
        {
          "name": "Tien Xuan Vo",
          "email": "tien.xuan.vo@gmail.com",
          "role": "Developer"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/tienvx/mbt-bundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "graphp/algorithms": "~0.8.1",
        "symfony/framework-bundle": "~4.2.0",
        "symfony/console": "~4.2.0",
        "symfony/workflow": "~4.2.0",
        "symfony/validator": "~4.2.0",
        "symfony/messenger": "~4.2.0",
        "symfony/process": "~4.2.0",
        "symfony/serializer": "~4.2.0",
        "doctrine/doctrine-bundle": "~1.10.0",
        "doctrine/orm": "~2.6.0",
        "dunglas/doctrine-json-odm": "~0.1.3",
        "symfony/monolog-bundle": "~3.3.0",
        "oneup/flysystem-bundle": "~3.0.0",
        "symfony/cache": "~4.2.0",
        "symfony/config": "~4.2.0",
        "symfony/debug": "~4.2.0",
        "symfony/dependency-injection": "~4.2.0",
        "symfony/event-dispatcher": "~4.2.0",
        "symfony/filesystem": "~4.2.0",
        "symfony/finder": "~4.2.0",
        "symfony/http-foundation": "~4.2.0",
        "symfony/http-kernel": "~4.2.0",
        "symfony/inflector": "~4.2.0",
        "symfony/monolog-bridge": "~4.2.0",
        "symfony/property-access": "~4.2.0",
        "symfony/property-info": "~4.2.0",
        "symfony/routing": "~4.2.0",
        "symfony/var-exporter": "~4.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4.0",
        "php-coveralls/php-coveralls": "~2.1.0",
        "friendsofphp/php-cs-fixer": "~2.14.0",
        "symfony/yaml": "~4.2.0",
        "symfony/expression-language": "~4.2.0",
        "symfony/security-bundle": "~4.2.0",
        "symfony/maker-bundle": "~1.11.0",
        "tienvx/messenger-memory-transport": "~0.1.0",
        "league/flysystem-memory": "~1.0.0",
        "symfony/options-resolver": "~4.2.0",
        "symfony/security-core": "~4.2.0",
        "symfony/security-csrf": "~4.2.0",
        "symfony/security-guard": "~4.2.0",
        "symfony/security-http": "~4.2.0",
        "symfony/stopwatch": "~4.2.0"
    },
    "autoload": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\Tests\\": "tests/",
            "App\\": "tests/app/src/"
        }
    },
    "scripts": {
        "coverage": "php vendor/bin/phpunit --coverage-clover tests/logs/clover.xml --colors=always",
        "cs-fix": "php vendor/bin/php-cs-fixer fix --dry-run --ansi",
        "coverall": "php vendor/bin/php-coveralls",
        "test": [
            "php tests/app/bin/console cache:clear --env=test --ansi",
            "php vendor/bin/phpunit --colors=always"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<4.0",
        "symfony/debug": "<4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6-dev"
        }
    },
    "suggest": {
        "ext-amqp": "Needed to support amqp transport"
    }
}
